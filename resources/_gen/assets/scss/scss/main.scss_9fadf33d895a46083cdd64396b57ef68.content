/*------------------------------------*\
    #ABSTRACTS: COLORS
\*------------------------------------*/
:root {
  --bg-opacity: 1;
  --color-dark: #151515;
  --color-body: #f5f5f5;
  --color-light: #767676;
  --color-lighter: #888;
  --color-lightest: #f5f5f5;
  --color-link: #50248F;
  --color-teal: #287088;
  --color-sea: #396ca0;
  --color-purple: #542bff;
  /* #542bff in decimal RGB */
  --color-link-rgb: 84, 43, 255;
  --color-white: #fff;
  --color-1: #fff;
  --color-2: #000;
  --color-3: #f0efef;
  --color-4: hsla(0, 0%, 33%, 0.15);
  --color-5: #232323;
  --color-6: #4e4d4d;
  --color-7: #b4b4b4;
  --grid-width: 140rem;
  --logo-rotation-duration: 15s;
  --left-margin: calc(var(--grid-width)/12*1); }

